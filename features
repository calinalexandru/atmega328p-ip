MODEL 3 (ATMEGA328), VERSION 1, REVISION 1

*** COMMANDS ***

*** EEPROM ***

	0-1		multiplier temp sensor 0
	2-3		offset temp sensor 0
	...
	28-29	multiplier temp sensor 7
	30-31	offset temp sensor 7

	32		mac address octet 0
	33		mac address octet 1
	34		mac address octet 2
	35		mac address octet 3
	36		mac address octet 4
	37		mac address octet 5

	38		ipv4 address byte 0
	39		ipv4 address byte 1
	40		ipv4 address byte 2
	41		ipv4 address byte 3

*** FEATURES (BOARD 0, TEST) ***

		PORT	SPI		TWI		I/O

	1	c6		N/C				RESET
	2	d0						d0		output	status led 0 life	small transparent green
	3	d1						d1		output	status led 1 cmd	small transparent red
	4	d2						int0	input	enc28j60 int
	5	d3						oc2b	output						rectangular green			output 4
	6	d4		 				d4		output	status led 2 i2c	small red
	7	Vcc
	8	GND
	9	b6						b6		output	status led 3 spi	small orange
	10	b7						b7		input	digital input									input 0
	11	d5						oc0b	output	timer 0 pwm b		large green					output 1
	12	d6						oc0a	output	timer 0 pwm a		large red					output 0
	13	d7		N/C
	14	b0		/ss													square green

	15	b1						oc1a	output timer 1 pwm a		small red					output 2
	16	b2						oc1b	output timer 1 pwm b		small green					output 3
	17	b3		mosi												square green
	18	b4		miso												square red
	19	b5		sck													square red
	20	AVcc
	21	aref
	22	GND
	23	c0						adc0	input	analog input									analog input 0
	24	c1		N/C
	25	c2		N/C
	26	c3		N/C
	27	c4				sda
	28	c5				scl
