MODEL 3 (ATMEGA328), VERSION 1, REVISION 1

*** COMMANDS ***

*** EEPROM ***

	0-1		multiplier temp sensor 0
	2-3		offset temp sensor 0
	...
	28-29	multiplier temp sensor 7
	30-31	offset temp sensor 7

	32		mac address octet 0
	33		mac address octet 1
	34		mac address octet 2
	35		mac address octet 3
	36		mac address octet 4
	37		mac address octet 5

	38		ipv4 address byte 0
	39		ipv4 address byte 1
	40		ipv4 address byte 2
	41		ipv4 address byte 3

*** FEATURES ***

		PORT	SPI		TWI		I/O

	1	c6						RESET
	2	d0						d0		small transparent green		led heartbeat
	3	d1						d1		small transparent red		led command
	4	d2						int0								enc int
	5	d3						d3		rectangular green			DEBUG 1
	6	d4		 				d4		small red					led i2c
	7	Vcc															VCC
	8	GND															GND
	9	b6						b6		small orange				led spi
	10	b7						b7									button light down
	11	d5						d5		large green					DEBUG 2
	12	d6						d6		large red					DEBUG 3
	13	d7						d7									button light up
	14	b0		/cs				b0		square green				spi ss

	28	c5				scl											i2c scl
	27	c4				sda											i2c sda
	26	c3						c3									*
	25	c2						c2									*
	24	c1						c1									*
	23	c0						adc0								tmp36
	22	GND															GND
	21	AREF														AREF
	20	AVcc														AVCC
	19	b5		sck						square red					spi sck
	18	b4		miso					square red					spi miso
	17	b3		mosi					square green				spi mosi
	16	b2						oc1b	small green					ir
	15	b1						oc1a	small red					light
